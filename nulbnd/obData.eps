object Square {
  var x, y;
  var unit: TrgUnit;
  function constructor (x: EUDVariable, y: EUDVariable, unit: TrgUnit) {
    this.x = x;
    this.y = y;
    this.unit = unit;
  }
};

object Step {
  var squares: EUDArray;
  var numSquares, wait;
  function constructor (squares: EUDArray, num: EUDVariable, wait: EUDVariable) {
    this.squares = squares;
    this.numSquares = num;
    this.wait = wait;
  }
};

object Obstacle {
  var terrainX: EUDArray;
  var terrainY: EUDArray;
  var steps: EUDArray;
  var numSteps, numTerrain;
  function constructor (terrainX: EUDArray, terrainY: EUDArray, numTerrain: EUDArray,
      steps: EUDArray, num: EUDVariable) {
    this.terrainX = terrainX;
    this.terrainY = terrainY;
    this.steps = steps;
    this.numSteps = num;
    this.numTerrain = numTerrain;
  }
};

